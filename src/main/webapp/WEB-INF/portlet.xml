<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0"
             xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">
<portlet>
    <portlet-name>VaadinPortlet</portlet-name>
    <display-name>VaadinPortlet</display-name>
    <portlet-class>VaadinPortlet.VaadinPortletPortlet</portlet-class>
    <init-param>
        <name>widgetset</name>
        <value>VaadinPortletWidgetSet</value>
    </init-param>
    <init-param>
        <name>UI</name>
        <value>VaadinPortlet.MyVaadinUI</value>
    </init-param>
    <expiration-cache>0</expiration-cache>
    <supports>
        <mime-type>text/html</mime-type>
        <portlet-mode>view</portlet-mode>
    </supports>
    <portlet-info>
        <title>Vaadin RSS Portlet</title>
        <short-title>VRSSPortlet</short-title>
        <keywords>Vaadin RSS Portlet</keywords>
    </portlet-info>
    <security-role-ref>
        <role-name>administrator</role-name>
    </security-role-ref>
    <security-role-ref>
        <role-name>guest</role-name>
    </security-role-ref>
    <security-role-ref>
        <role-name>power-user</role-name>
    </security-role-ref>
    <security-role-ref>
        <role-name>user</role-name>
    </security-role-ref>
</portlet>
</portlet-app>